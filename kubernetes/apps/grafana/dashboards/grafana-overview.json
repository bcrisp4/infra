{
  "panels": [
    {
      "collapsed": false,
      "gridPos": { "h": 1, "w": 24, "x": 0, "y": 0 },
      "title": "Overview",
      "type": "row"
    },
    {
      "datasource": { "type": "prometheus", "uid": "${datasource}" },
      "fieldConfig": {
        "defaults": {
          "color": { "mode": "thresholds" },
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "color": "red", "value": null },
              { "color": "yellow", "value": 3600 },
              { "color": "green", "value": 86400 }
            ]
          },
          "unit": "s"
        }
      },
      "gridPos": { "h": 4, "w": 3, "x": 0, "y": 1 },
      "options": { "reduceOptions": { "calcs": ["lastNotNull"] } },
      "targets": [
        {
          "expr": "time() - process_start_time_seconds{job=\"$job\", instance=~\"$instance\"}",
          "legendFormat": "{{instance}}"
        }
      ],
      "title": "Uptime",
      "type": "stat"
    },
    {
      "datasource": { "type": "prometheus", "uid": "${datasource}" },
      "fieldConfig": {
        "defaults": {
          "color": { "mode": "thresholds" },
          "thresholds": {
            "mode": "absolute",
            "steps": [{ "color": "blue", "value": null }]
          }
        }
      },
      "gridPos": { "h": 4, "w": 3, "x": 3, "y": 1 },
      "options": {
        "reduceOptions": { "calcs": ["lastNotNull"] },
        "textMode": "name"
      },
      "targets": [
        {
          "expr": "grafana_build_info{job=\"$job\", instance=~\"$instance\"}",
          "legendFormat": "{{version}}"
        }
      ],
      "title": "Version",
      "type": "stat"
    },
    {
      "datasource": { "type": "prometheus", "uid": "${datasource}" },
      "fieldConfig": {
        "defaults": {
          "color": { "mode": "thresholds" },
          "thresholds": {
            "mode": "absolute",
            "steps": [{ "color": "green", "value": null }]
          }
        }
      },
      "gridPos": { "h": 4, "w": 3, "x": 6, "y": 1 },
      "options": { "reduceOptions": { "calcs": ["lastNotNull"] } },
      "targets": [
        {
          "expr": "grafana_stat_active_users{job=\"$job\", instance=~\"$instance\"}",
          "legendFormat": ""
        }
      ],
      "title": "Active Users",
      "type": "stat"
    },
    {
      "datasource": { "type": "prometheus", "uid": "${datasource}" },
      "fieldConfig": {
        "defaults": {
          "color": { "mode": "thresholds" },
          "thresholds": {
            "mode": "absolute",
            "steps": [{ "color": "purple", "value": null }]
          }
        }
      },
      "gridPos": { "h": 4, "w": 3, "x": 9, "y": 1 },
      "options": { "reduceOptions": { "calcs": ["lastNotNull"] } },
      "targets": [
        {
          "expr": "grafana_stat_totals_dashboard{job=\"$job\", instance=~\"$instance\"}",
          "legendFormat": ""
        }
      ],
      "title": "Dashboards",
      "type": "stat"
    },
    {
      "datasource": { "type": "prometheus", "uid": "${datasource}" },
      "fieldConfig": {
        "defaults": {
          "color": { "mode": "thresholds" },
          "thresholds": {
            "mode": "absolute",
            "steps": [{ "color": "orange", "value": null }]
          }
        }
      },
      "gridPos": { "h": 4, "w": 3, "x": 12, "y": 1 },
      "options": { "reduceOptions": { "calcs": ["lastNotNull"] } },
      "targets": [
        {
          "expr": "grafana_stat_totals_datasource{job=\"$job\", instance=~\"$instance\"}",
          "legendFormat": ""
        }
      ],
      "title": "Datasources",
      "type": "stat"
    },
    {
      "datasource": { "type": "prometheus", "uid": "${datasource}" },
      "fieldConfig": {
        "defaults": {
          "color": { "mode": "thresholds" },
          "thresholds": {
            "mode": "absolute",
            "steps": [{ "color": "red", "value": null }]
          }
        }
      },
      "gridPos": { "h": 4, "w": 3, "x": 15, "y": 1 },
      "options": { "reduceOptions": { "calcs": ["lastNotNull"] } },
      "targets": [
        {
          "expr": "grafana_stat_totals_alert_rules{job=\"$job\", instance=~\"$instance\"}",
          "legendFormat": ""
        }
      ],
      "title": "Alert Rules",
      "type": "stat"
    },
    {
      "datasource": { "type": "prometheus", "uid": "${datasource}" },
      "fieldConfig": {
        "defaults": {
          "color": { "mode": "thresholds" },
          "thresholds": {
            "mode": "absolute",
            "steps": [{ "color": "blue", "value": null }]
          }
        }
      },
      "gridPos": { "h": 4, "w": 3, "x": 18, "y": 1 },
      "options": { "reduceOptions": { "calcs": ["lastNotNull"] } },
      "targets": [
        {
          "expr": "grafana_stat_totals_folder{job=\"$job\", instance=~\"$instance\"}",
          "legendFormat": ""
        }
      ],
      "title": "Folders",
      "type": "stat"
    },
    {
      "datasource": { "type": "prometheus", "uid": "${datasource}" },
      "fieldConfig": {
        "defaults": {
          "color": { "mode": "thresholds" },
          "thresholds": {
            "mode": "absolute",
            "steps": [{ "color": "green", "value": null }]
          }
        }
      },
      "gridPos": { "h": 4, "w": 3, "x": 21, "y": 1 },
      "options": { "reduceOptions": { "calcs": ["lastNotNull"] } },
      "targets": [
        {
          "expr": "grafana_stat_total_users{job=\"$job\", instance=~\"$instance\"}",
          "legendFormat": ""
        }
      ],
      "title": "Total Users",
      "type": "stat"
    },
    {
      "collapsed": false,
      "gridPos": { "h": 1, "w": 24, "x": 0, "y": 5 },
      "title": "HTTP Performance",
      "type": "row"
    },
    {
      "datasource": { "type": "prometheus", "uid": "${datasource}" },
      "fieldConfig": {
        "defaults": {
          "custom": {
            "drawStyle": "line",
            "fillOpacity": 10,
            "lineWidth": 1,
            "stacking": { "mode": "none" }
          },
          "unit": "reqps"
        }
      },
      "gridPos": { "h": 8, "w": 8, "x": 0, "y": 6 },
      "options": {
        "legend": {
          "calcs": ["mean", "max"],
          "displayMode": "table",
          "placement": "bottom"
        }
      },
      "targets": [
        {
          "expr": "sum(rate(grafana_http_request_duration_seconds_count{job=\"$job\", instance=~\"$instance\"}[$__rate_interval])) by (handler)",
          "legendFormat": "{{handler}}"
        }
      ],
      "title": "HTTP Request Rate",
      "type": "timeseries"
    },
    {
      "datasource": { "type": "prometheus", "uid": "${datasource}" },
      "fieldConfig": {
        "defaults": {
          "custom": { "drawStyle": "line", "fillOpacity": 10, "lineWidth": 1 },
          "unit": "s"
        }
      },
      "gridPos": { "h": 8, "w": 8, "x": 8, "y": 6 },
      "options": {
        "legend": {
          "calcs": ["mean", "max"],
          "displayMode": "table",
          "placement": "bottom"
        }
      },
      "targets": [
        {
          "expr": "histogram_quantile(0.95, sum(rate(grafana_http_request_duration_seconds_bucket{job=\"$job\", instance=~\"$instance\"}[$__rate_interval])) by (le, handler))",
          "legendFormat": "{{handler}}"
        }
      ],
      "title": "HTTP Request Latency (p95)",
      "type": "timeseries"
    },
    {
      "datasource": { "type": "prometheus", "uid": "${datasource}" },
      "fieldConfig": {
        "defaults": {
          "custom": { "drawStyle": "line", "fillOpacity": 10, "lineWidth": 1 },
          "unit": "short"
        }
      },
      "gridPos": { "h": 8, "w": 8, "x": 16, "y": 6 },
      "options": {
        "legend": {
          "calcs": ["mean", "max"],
          "displayMode": "table",
          "placement": "bottom"
        }
      },
      "targets": [
        {
          "expr": "grafana_http_request_in_flight{job=\"$job\", instance=~\"$instance\"}",
          "legendFormat": "{{handler}}"
        }
      ],
      "title": "HTTP Requests In Flight",
      "type": "timeseries"
    },
    {
      "datasource": { "type": "prometheus", "uid": "${datasource}" },
      "fieldConfig": {
        "defaults": {
          "custom": {
            "drawStyle": "line",
            "fillOpacity": 10,
            "lineWidth": 1,
            "stacking": { "mode": "normal" }
          },
          "unit": "reqps"
        }
      },
      "gridPos": { "h": 8, "w": 8, "x": 0, "y": 14 },
      "options": {
        "legend": {
          "calcs": ["mean", "max"],
          "displayMode": "table",
          "placement": "bottom"
        }
      },
      "targets": [
        {
          "expr": "sum(rate(grafana_api_response_status_total{job=\"$job\", instance=~\"$instance\"}[$__rate_interval])) by (status_code)",
          "legendFormat": "{{status_code}}"
        }
      ],
      "title": "HTTP Response Status",
      "type": "timeseries"
    },
    {
      "datasource": { "type": "prometheus", "uid": "${datasource}" },
      "fieldConfig": {
        "defaults": {
          "custom": { "drawStyle": "line", "fillOpacity": 10, "lineWidth": 1 },
          "max": 1,
          "min": 0,
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "color": "green", "value": null },
              { "color": "yellow", "value": 0.01 },
              { "color": "red", "value": 0.05 }
            ]
          },
          "unit": "percentunit"
        }
      },
      "gridPos": { "h": 8, "w": 8, "x": 8, "y": 14 },
      "options": {
        "legend": {
          "calcs": ["mean", "max"],
          "displayMode": "table",
          "placement": "bottom"
        }
      },
      "targets": [
        {
          "expr": "sum(rate(grafana_api_response_status_total{job=\"$job\", instance=~\"$instance\", status_code=~\"4..|5..\"}[$__rate_interval])) / sum(rate(grafana_api_response_status_total{job=\"$job\", instance=~\"$instance\"}[$__rate_interval]))",
          "legendFormat": "Error Rate"
        }
      ],
      "title": "HTTP Error Rate (4xx/5xx)",
      "type": "timeseries"
    },
    {
      "datasource": { "type": "prometheus", "uid": "${datasource}" },
      "fieldConfig": {
        "defaults": {
          "custom": { "drawStyle": "line", "fillOpacity": 10, "lineWidth": 1 },
          "unit": "bytes"
        }
      },
      "gridPos": { "h": 8, "w": 8, "x": 16, "y": 14 },
      "options": {
        "legend": {
          "calcs": ["mean", "max"],
          "displayMode": "table",
          "placement": "bottom"
        }
      },
      "targets": [
        {
          "expr": "histogram_quantile(0.95, sum(rate(grafana_http_response_size_bytes_bucket{job=\"$job\", instance=~\"$instance\"}[$__rate_interval])) by (le, handler))",
          "legendFormat": "p95 {{handler}}"
        }
      ],
      "title": "HTTP Response Size",
      "type": "timeseries"
    },
    {
      "collapsed": false,
      "gridPos": { "h": 1, "w": 24, "x": 0, "y": 22 },
      "title": "Compute & Storage Resources",
      "type": "row"
    },
    {
      "datasource": { "type": "prometheus", "uid": "${datasource}" },
      "fieldConfig": {
        "defaults": {
          "custom": { "drawStyle": "line", "fillOpacity": 10, "lineWidth": 1 },
          "unit": "short"
        }
      },
      "gridPos": { "h": 8, "w": 6, "x": 0, "y": 23 },
      "options": { "legend": { "displayMode": "list", "placement": "bottom" } },
      "targets": [
        {
          "expr": "rate(grafana_process_cpu_seconds_total{job=\"$job\", instance=~\"$instance\"}[$__rate_interval])",
          "legendFormat": "{{instance}}"
        }
      ],
      "title": "CPU Usage",
      "type": "timeseries"
    },
    {
      "datasource": { "type": "prometheus", "uid": "${datasource}" },
      "fieldConfig": {
        "defaults": {
          "custom": { "drawStyle": "line", "fillOpacity": 10, "lineWidth": 1 },
          "unit": "bytes"
        }
      },
      "gridPos": { "h": 8, "w": 6, "x": 6, "y": 23 },
      "options": { "legend": { "displayMode": "list", "placement": "bottom" } },
      "targets": [
        {
          "expr": "grafana_process_resident_memory_bytes{job=\"$job\", instance=~\"$instance\"}",
          "legendFormat": "Resident"
        },
        {
          "expr": "grafana_process_virtual_memory_bytes{job=\"$job\", instance=~\"$instance\"}",
          "legendFormat": "Virtual"
        }
      ],
      "title": "Memory Usage",
      "type": "timeseries"
    },
    {
      "datasource": { "type": "prometheus", "uid": "${datasource}" },
      "fieldConfig": {
        "defaults": {
          "custom": { "drawStyle": "line", "fillOpacity": 10, "lineWidth": 1 },
          "unit": "short"
        }
      },
      "gridPos": { "h": 8, "w": 6, "x": 12, "y": 23 },
      "options": { "legend": { "displayMode": "list", "placement": "bottom" } },
      "targets": [
        {
          "expr": "grafana_process_open_fds{job=\"$job\", instance=~\"$instance\"}",
          "legendFormat": "Open FDs"
        },
        {
          "expr": "grafana_process_max_fds{job=\"$job\", instance=~\"$instance\"}",
          "legendFormat": "Max FDs"
        }
      ],
      "title": "Open File Descriptors",
      "type": "timeseries"
    },
    {
      "datasource": { "type": "prometheus", "uid": "${datasource}" },
      "fieldConfig": {
        "defaults": {
          "custom": { "drawStyle": "line", "fillOpacity": 10, "lineWidth": 1 },
          "unit": "Bps"
        }
      },
      "gridPos": { "h": 8, "w": 6, "x": 18, "y": 23 },
      "options": { "legend": { "displayMode": "list", "placement": "bottom" } },
      "targets": [
        {
          "expr": "rate(grafana_process_network_receive_bytes_total{job=\"$job\", instance=~\"$instance\"}[$__rate_interval])",
          "legendFormat": "Receive"
        },
        {
          "expr": "rate(grafana_process_network_transmit_bytes_total{job=\"$job\", instance=~\"$instance\"}[$__rate_interval])",
          "legendFormat": "Transmit"
        }
      ],
      "title": "Network I/O",
      "type": "timeseries"
    },
    {
      "collapsed": false,
      "gridPos": { "h": 1, "w": 24, "x": 0, "y": 31 },
      "title": "Database",
      "type": "row"
    },
    {
      "datasource": { "type": "prometheus", "uid": "${datasource}" },
      "fieldConfig": {
        "defaults": {
          "custom": { "drawStyle": "line", "fillOpacity": 10, "lineWidth": 1 },
          "unit": "short"
        }
      },
      "gridPos": { "h": 8, "w": 8, "x": 0, "y": 32 },
      "options": {
        "legend": {
          "calcs": ["mean", "max"],
          "displayMode": "table",
          "placement": "bottom"
        }
      },
      "targets": [
        {
          "expr": "grafana_database_conn_open{job=\"$job\", instance=~\"$instance\"}",
          "legendFormat": "Open"
        },
        {
          "expr": "grafana_database_conn_in_use{job=\"$job\", instance=~\"$instance\"}",
          "legendFormat": "In Use"
        },
        {
          "expr": "grafana_database_conn_idle{job=\"$job\", instance=~\"$instance\"}",
          "legendFormat": "Idle"
        },
        {
          "expr": "grafana_database_conn_max_open{job=\"$job\", instance=~\"$instance\"}",
          "legendFormat": "Max Open"
        }
      ],
      "title": "Database Connections",
      "type": "timeseries"
    },
    {
      "datasource": { "type": "prometheus", "uid": "${datasource}" },
      "fieldConfig": {
        "defaults": {
          "custom": { "drawStyle": "line", "fillOpacity": 10, "lineWidth": 1 },
          "unit": "s"
        }
      },
      "gridPos": { "h": 8, "w": 8, "x": 8, "y": 32 },
      "options": {
        "legend": {
          "calcs": ["mean", "max"],
          "displayMode": "table",
          "placement": "bottom"
        }
      },
      "targets": [
        {
          "expr": "rate(grafana_database_conn_wait_duration_seconds{job=\"$job\", instance=~\"$instance\"}[$__rate_interval])",
          "legendFormat": "Wait Duration/s"
        },
        {
          "expr": "rate(grafana_database_conn_wait_count_total{job=\"$job\", instance=~\"$instance\"}[$__rate_interval])",
          "legendFormat": "Wait Count/s"
        }
      ],
      "title": "Database Connection Wait",
      "type": "timeseries"
    },
    {
      "datasource": { "type": "prometheus", "uid": "${datasource}" },
      "fieldConfig": {
        "defaults": {
          "custom": { "drawStyle": "line", "fillOpacity": 10, "lineWidth": 1 },
          "unit": "cps"
        }
      },
      "gridPos": { "h": 8, "w": 8, "x": 16, "y": 32 },
      "options": {
        "legend": {
          "calcs": ["mean", "max"],
          "displayMode": "table",
          "placement": "bottom"
        }
      },
      "targets": [
        {
          "expr": "rate(grafana_database_conn_max_idle_closed_total{job=\"$job\", instance=~\"$instance\"}[$__rate_interval])",
          "legendFormat": "Idle Timeout"
        },
        {
          "expr": "rate(grafana_database_conn_max_lifetime_closed_total{job=\"$job\", instance=~\"$instance\"}[$__rate_interval])",
          "legendFormat": "Lifetime"
        }
      ],
      "title": "Database Connections Closed",
      "type": "timeseries"
    },
    {
      "collapsed": false,
      "gridPos": { "h": 1, "w": 24, "x": 0, "y": 40 },
      "title": "Alerting",
      "type": "row"
    },
    {
      "datasource": { "type": "prometheus", "uid": "${datasource}" },
      "fieldConfig": {
        "defaults": {
          "color": { "mode": "thresholds" },
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "color": "green", "value": null },
              { "color": "yellow", "value": 5 },
              { "color": "red", "value": 10 }
            ]
          }
        }
      },
      "gridPos": { "h": 4, "w": 4, "x": 0, "y": 41 },
      "options": { "reduceOptions": { "calcs": ["lastNotNull"] } },
      "targets": [
        {
          "expr": "sum(grafana_alerting_active_alerts{job=\"$job\", instance=~\"$instance\"})",
          "legendFormat": ""
        }
      ],
      "title": "Active Alerts",
      "type": "stat"
    },
    {
      "datasource": { "type": "prometheus", "uid": "${datasource}" },
      "fieldConfig": {
        "defaults": {
          "color": { "mode": "thresholds" },
          "thresholds": {
            "mode": "absolute",
            "steps": [{ "color": "blue", "value": null }]
          }
        }
      },
      "gridPos": { "h": 4, "w": 4, "x": 4, "y": 41 },
      "options": { "reduceOptions": { "calcs": ["lastNotNull"] } },
      "targets": [
        {
          "expr": "sum(grafana_alerting_rule_groups{job=\"$job\", instance=~\"$instance\"})",
          "legendFormat": ""
        }
      ],
      "title": "Rule Groups",
      "type": "stat"
    },
    {
      "datasource": { "type": "prometheus", "uid": "${datasource}" },
      "fieldConfig": {
        "defaults": {
          "color": { "mode": "thresholds" },
          "thresholds": {
            "mode": "absolute",
            "steps": [{ "color": "purple", "value": null }]
          }
        }
      },
      "gridPos": { "h": 4, "w": 4, "x": 8, "y": 41 },
      "options": { "reduceOptions": { "calcs": ["lastNotNull"] } },
      "targets": [
        {
          "expr": "sum(grafana_alerting_schedule_alert_rules{job=\"$job\", instance=~\"$instance\"})",
          "legendFormat": ""
        }
      ],
      "title": "Scheduled Rules",
      "type": "stat"
    },
    {
      "datasource": { "type": "prometheus", "uid": "${datasource}" },
      "fieldConfig": {
        "defaults": {
          "color": { "mode": "thresholds" },
          "thresholds": {
            "mode": "absolute",
            "steps": [{ "color": "orange", "value": null }]
          }
        }
      },
      "gridPos": { "h": 4, "w": 4, "x": 12, "y": 41 },
      "options": { "reduceOptions": { "calcs": ["lastNotNull"] } },
      "targets": [
        {
          "expr": "sum(grafana_alerting_silences{job=\"$job\", instance=~\"$instance\", state=\"active\"})",
          "legendFormat": ""
        }
      ],
      "title": "Active Silences",
      "type": "stat"
    },
    {
      "datasource": { "type": "prometheus", "uid": "${datasource}" },
      "fieldConfig": {
        "defaults": {
          "color": { "mode": "thresholds" },
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "color": "green", "value": null },
              { "color": "yellow", "value": 5 },
              { "color": "red", "value": 30 }
            ]
          },
          "unit": "s"
        }
      },
      "gridPos": { "h": 4, "w": 4, "x": 16, "y": 41 },
      "options": { "reduceOptions": { "calcs": ["lastNotNull"] } },
      "targets": [
        {
          "expr": "grafana_alerting_scheduler_behind_seconds{job=\"$job\", instance=~\"$instance\"}",
          "legendFormat": ""
        }
      ],
      "title": "Scheduler Behind",
      "type": "stat"
    },
    {
      "datasource": { "type": "prometheus", "uid": "${datasource}" },
      "fieldConfig": {
        "defaults": {
          "color": { "mode": "thresholds" },
          "thresholds": {
            "mode": "absolute",
            "steps": [{ "color": "blue", "value": null }]
          },
          "unit": "s"
        }
      },
      "gridPos": { "h": 4, "w": 4, "x": 20, "y": 41 },
      "options": { "reduceOptions": { "calcs": ["lastNotNull"] } },
      "targets": [
        {
          "expr": "grafana_alerting_ticker_interval_seconds{job=\"$job\", instance=~\"$instance\"}",
          "legendFormat": ""
        }
      ],
      "title": "Ticker Interval",
      "type": "stat"
    },
    {
      "datasource": { "type": "prometheus", "uid": "${datasource}" },
      "fieldConfig": {
        "defaults": {
          "custom": { "drawStyle": "line", "fillOpacity": 10, "lineWidth": 1 },
          "unit": "ops"
        }
      },
      "gridPos": { "h": 8, "w": 8, "x": 0, "y": 45 },
      "options": {
        "legend": {
          "calcs": ["mean", "max"],
          "displayMode": "table",
          "placement": "bottom"
        }
      },
      "targets": [
        {
          "expr": "sum(rate(grafana_alerting_rule_evaluations_total{job=\"$job\", instance=~\"$instance\"}[$__rate_interval]))",
          "legendFormat": "Total"
        },
        {
          "expr": "sum(rate(grafana_alerting_rule_evaluation_failures_total{job=\"$job\", instance=~\"$instance\"}[$__rate_interval]))",
          "legendFormat": "Failures"
